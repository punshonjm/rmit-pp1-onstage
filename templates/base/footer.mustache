	<!-- Control Center for Material Kit-->
	<script src="/public/js/core/material-kit.js?v=2.0.4" type="text/javascript"></script>
	<script src="/public/js/core/app.js" type="text/javascript"></script>

	{{#if this.user.golden_ticket}}
	<script>
		$(document).ready(function() {} ).on("click", ".changeUserType", function() {
			window.app.swapType( $(this) );
		});

		window.app.swapType = function($this) {
			$(".changeUserType").prop("disabled", true);
			$.post("/api/user/golden_ticket/" + $this.data().type, function() {
				window.location.reload();
			}).fail(function() {
				$(".changeUser").prop("disabled", false);
			});
		};
	</script>
	{{/if}}
